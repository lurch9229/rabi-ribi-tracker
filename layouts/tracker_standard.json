{
  "tracker_capture_item": {
    "type": "container",
    "content": {
      "type": "itemgrid",
      "h_alignment": "center",
      "item_margin": "1,2",
      "rows": [
        [ "pack", "health", "attack", "mana", "regen", "nature", "fire", "water", "light"],
        [ "hammer", "bombs", "amulet", "super", "slippers", "doublejump", "slide, bunnystrike"],
        [ "walljump", "airdash", "triplejump", "whirl", "hammerSlide", "hammerRoll", "spikebarrier"],
        [ "hammerWave", "sunnybeam", "chaosrod", "healingstaff", "explodeshot", "carrotshot", "eggshot"],
        [ "trick","chapter","easterEgg","tm", "knowledge"]
      ]
    }
  },

  "tracker_horizontal": 
  {
    "type": "container",
    "content": 
    {
      "type": "dock",
      "dropshadow": true,
      "content": 
      [
        {
          "type": "array",
          "dock": "bottom",
          "orientation": "horizontal",
          "content": 
          [
            {
              "type": "group",
              "header": "Items",
              "content": 
              {
                "type": "array",
                "orientation": "vertical",
                "margin": "0,0",
                "content": 
                [
                  {
                    "type": "layout",
                    "key": "item_grid"
                  }
                ]
              }
            },
            {
              "type": "group",
              "header": "Bosses",
              "content":
              {
                "type": "array",
                "orientation": "vertical",
                "margin": "0,0",
                "content":
                [
                  {
                    "type": "layout",
                    "key": "rabi_boss_grid"
                  }
                ]
              }
            },
            {
              "type": "group",
              "header": "Badges",
              "content":
              {
                "type": "array",
                "orientation": "vertical",
                "margin": "0,0",
                "content":
                [
                  {
                    "type": "layout",
                    "key": "badge_grid"
                  }
                ]
              }
            }
          ]
        },
        {
          "type": "tabbed",
          "tabs": 
          [
            {
              "title": "Main Island",
              "content": 
              {
                "type": "layout",
                "key": "main_island_maps"
              }
            },
            {
              "title": "Side Areas",
              "content": 
              {
                "type": "layout",
                "key": "side_areas_maps"
              }
            }
          ]
        }
      ]
    }
  },

  "tracker_vertical":
  {
    "type": "container",
    "content": 
    {
      "type": "dock",
      "dropshadow": true,
      "content": 
      [
        {
          "type": "dock",
          "dock": "bottom",
          "h_alignment": "stretch",
          "content": 
          [
            {
              "type": "array",
              "orientation": "vertical",
              "content": 
              [
                {
                  "type": "group",
                  "header": "Items",
                  "content": {
                    "type": "array",
                    "orientation": "horizontal",
                    "margin": "0,0",
                    "content": 
                    [
                      {
                        "type": "layout",
                        "key": "rabi_item_grid"
                      }
                    ]
                  }
                },
                {
                  "type": "group",
                  "header": "Bosses",
                  "content":
                  {
                    "type": "array",
                    "orientation": "horizontal",
                    "margin": "0,0",
                    "content":
                    [
                      {
                        "type": "layout",
                        "key": "rabi_boss_grid"
                      }
                    ]
                  }
                },
                {
                  "type": "group",
                  "header": "Pinned Locations",
                  "content": 
                  {
                    "type": "recentpins",
                    "style": "wrap",
                    "h_alignment": "stretch",
                    "v_alignment": "stretch",
                    "orientation": "horizontal",
                    "compact": true
                  }
                }
              ]
            }
          ]
        },
        {
          "type": "tabbed",
          "tabs": [
            {
              "title": "Main Island",
              "content": {
                "type": "layout",
                "key": "main_island_maps"
              }
            },
            {
              "title": "Side Areas",
              "content": {
                "type": "layout",
                "key": "side_areas_maps"
              }
            }
          ]
        }
      ]
    }
  }
}
